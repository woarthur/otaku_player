<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <!--通过 AppSwitch 禁用 WPF 内置的触摸让 WPF 程序可以处理 Windows 触摸消息-->
  <!--https://blog.walterlv.com/post/wpf-disable-stylus-and-touch-support.html#%E5%9C%A8%E9%80%8F%E6%98%8E%E7%AA%97%E5%8F%A3%E4%B8%8A%E8%A7%A6%E6%91%B8%E4%BC%9A%E6%8C%A1%E4%BD%8F-uwp-%E7%A8%8B%E5%BA%8F-->
  <runtime>
    <AppContextSwitchOverrides value="Switch.System.Windows.Input.Stylus.DisableStylusAndTouchSupport=true" />
  </runtime>
</configuration>